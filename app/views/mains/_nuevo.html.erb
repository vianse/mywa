
<div class="row">

   <br>
  <p>
    <%= link_to 'Registrar nueva tarjeta', codigos_path , :class => "button button-primary middle" %>
  </p>
  <br>
  <div class="panel">
    <h3>Listado de mis tarjetas</h3>
  </div>
 

<table class="table">
  <thead class="sinborde">
  </thead>
  <tbody>
    <% @mains.each do |main| %>
        <div class="four columns">
        <div class="callout-card swift">
          <div class="callout-card-icon">
              <%= link_to main.code, main, :class=> "code"%>
          </div>
          <div class="name">
            <%= main.name%>
          </div>
           
          <%= link_to 'Editar', edit_main_path(main), :class => "button button-primary small card_edit" %>
          <%= link_to 'Dar de baja', main, method: :delete, data: { confirm: 'Are you sure?' }, :class=>"button button-primary small delete" %>
        </div>
      </div>
    <% end %>
  </tbody>
</table>

<br>
</div>

